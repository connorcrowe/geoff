{
    "id": 8,
    "user_query": "Join attractions with wards by matching ward_id",
    "embedding_version": 0,
    "last_embedded": "",
    
    "metadata": {
        "tables": ["attractions", "wards"],
        "components": ["simple_select", "attribute_join"]
    },
    
    "plan": {
        "layers": [
            {
                "layer_name": "attractions_with_wards",
                "layer_type": "primary",
                "query": {
                    "type": "select",
                    "table": "attractions",
                    "alias": "a",
                    "distinct": true,
                    "columns": [
                        {"name": "a.name", "alias": "attraction_name"},
                        {"name": "a.category"},
                        {"name": "a.address"},
                        {"name": "w.name", "alias": "ward_name"},
                        {"name": "w.ward_id"},
                        {"name": "geometry", "expression": "ST_AsGeoJSON(a.geometry)", "alias": "geometry"}
                    ],
                    "joins": [
                        {
                            "type": "INNER",
                            "table": "wards",
                            "alias": "w",
                            "condition": {
                                "type": "attribute",
                                "left_column": "a.ward_id",
                                "right_column": "w.ward_id"
                            }
                        }
                    ]
                }
            }
        ]
    }
